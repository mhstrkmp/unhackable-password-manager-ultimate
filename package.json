  "description": "Simple App to learn some node.js",
  "main": "uhp.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
